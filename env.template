# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Database
# Use your Supabase PostgreSQL connection string
# Format: postgresql://postgres:password@host:port/database?schema=public
DATABASE_URL=postgresql://postgres:password@db.xxxxx.supabase.co:5432/postgres?schema=public

# Next.js
NODE_ENV=development

# Optional: Webhook Secret for Supabase webhooks
# SUPABASE_WEBHOOK_SECRET=your_webhook_secret_here

